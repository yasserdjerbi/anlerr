<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
    	<record id="group_return_reason_manager" model="res.groups">
	        <field name="name">Return Reasons Manager</field>
	        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
